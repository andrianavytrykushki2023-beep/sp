Program _ла;
Var 
  _A, _B, _C, _M: Int16;

Begin
  Get(_A);
  Get(_B);
  Get(_C);

  If _A >= _B And _A >= _C {
    _M -> _A;
  } Else If _B >= _A And _B >= _C {
    _M -> _B;
  } Else {
    _M -> _C;
  }

  Put(_M);

  If (_A Eg _B) And (_A Eg _C) And (_B Eg _C) {
    Put(1);
  } Else {
    Put(0);
  }

  If !(_A >= 0) Or !(_B >= 0) Or !(_C >= 0) {
    Put(-1);
  } Else {
    Put(0);
  }

  If _A >= (_B + _C) {
    Put(10);
  } Else {
    Put(0);
  }

End
